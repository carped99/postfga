# PostgreSQL Configuration
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=postgres
POSTGRES_PORT=5432

# PostFGA Extension Configuration
POSTFGA_AUTO_INSTALL=true
POSTFGA_ENDPOINT=dns:///openfga:8081
POSTFGA_STORE_ID=your_openfga_store_id
POSTFGA_RELATIONS=read,write,edit,delete,download,owner
POSTFGA_CACHE_TTL_MS=60000
POSTFGA_MAX_CACHE_ENTRIES=10000
POSTFGA_BGW_WORKERS=1
POSTFGA_FALLBACK_TO_GRPC=true

# OpenFGA Configuration
OPENFGA_HTTP_PORT=8080
OPENFGA_GRPC_PORT=8081
OPENFGA_PLAYGROUND_PORT=3000

# pgAdmin Configuration
PGADMIN_EMAIL=admin@example.com
PGADMIN_PASSWORD=admin
PGADMIN_PORT=5050
